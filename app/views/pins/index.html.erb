<div class="container">
<div id="pins" class="transition-enabled">
	<% @pins.each do |pin| %>
		<div class="box panel panel-default">
			<%= link_to (image_tag pin.image.url), pin %>

			<div class="panel-body">
				<h2> <%= link_to pin.title, pin %> </h2>
				<p class="user">
					Submitted By: <%= pin.user.email %>
				</p>
			</div>
		</div>
	<% end %>
</div>
</div>